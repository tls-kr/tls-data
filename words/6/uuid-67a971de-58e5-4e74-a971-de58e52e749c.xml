    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="PEACEFUL" tls:concept-id="uuid-e5801a73-4e38-4280-97cd-12d91ada17c6" xml:id="uuid-96921913-5a46-4041-ae7f-5a65a088ef94" n="26">
            <form corresp="#uuid-7459fdf6-00fc-4597-bbc5-219c10e39b4d">
            <orth>靜</orth>
            <pron xml:lang="zh-Latn-x-pinyin">jìng</pron>
