    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="ADD" tls:concept-id="uuid-92df29bf-6817-4406-82af-15188498b600" xml:id="uuid-f75728c3-940d-43cb-9da9-78aa2834cfb6" n="0">
            <form corresp="#uuid-78ac727e-ff44-4c6d-b9a2-b9ede88825c3">
<orth>并</orth>
                <pron xml:lang="zh-Latn-x-pinyin">bìng</pron>
