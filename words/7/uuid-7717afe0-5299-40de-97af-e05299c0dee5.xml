                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-76be1df4-3d73-4e5f-bbc2-729542645bc8">nab</tls:syn-func>
  <tls:sem-feat corresp="#uuid-2ef405b2-627b-4f29-940b-848d5428e30e">social</tls:sem-feat>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
  <def>abstract: friendship</def>
            </sense>
<sense xml:id="uuid-aa6333b3-6fa4-4478-a0e2-d1d812979310" n="7">
<gramGrp>
                    <pos>V</pos>
