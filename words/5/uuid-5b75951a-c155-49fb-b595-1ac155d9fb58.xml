    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="ENDURE" tls:concept-id="uuid-77b4a72e-50b0-47a2-832f-4906b8d3165b" xml:id="uuid-fc2feb7d-7dc3-4fb1-b7b0-abe97356fa53" n="27">
