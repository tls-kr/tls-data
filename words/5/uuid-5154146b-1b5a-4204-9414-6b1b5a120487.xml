<sense xml:id="uuid-122828ee-e5c2-4bc1-85a8-c282f4bbd44f" n="1">
<gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-53cee9f8-4041-45e5-ae55-f0bfdec33a11">vt[oN]</tls:syn-func>
  <tls:sem-feat corresp="#uuid-f55cff2f-f0e3-4f08-a89c-5d08fcf3fe89">act</tls:sem-feat>
                    <usg type="valuation">+</usg>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
  <def>make amends; make up for things, change oneself for the better; try to change to the better</def>
            </sense>
<sense xml:id="uuid-d8bb0927-646c-4ffd-a0b0-744161972a47" n="5">
<gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-e64a7a95-b54b-4c94-9d6d-f55dbf079701">vt(oN)</tls:syn-func>
                    <usg type="warring-states-currency">3</usg>
