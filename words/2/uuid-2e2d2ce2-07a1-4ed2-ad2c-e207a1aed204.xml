    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="GARMENT" tls:concept-id="uuid-de8e33db-0f66-48cc-8cb8-c9c783477528" xml:id="uuid-74babc30-c8df-41a6-a4cc-015de4674287" n="0">
