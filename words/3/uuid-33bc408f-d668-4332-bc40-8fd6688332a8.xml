                <pos>N</pos>
                <tls:syn-func corresp="#uuid-af99c48b-0543-4b6e-99c4-8b0543eb6ee9">npr.adN</tls:syn-func>
            </gramGrp>
            <def>of Yue4, Yue4-ish</def>
        </sense>
        <sense xml:id="uuid-775e9baa-30a6-4fdd-9e9b-aa30a6bfdd86" resp="#zadrapa" tls:created="2022-02-11T21:28:01.957+09:00" n="1">
