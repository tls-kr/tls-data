<gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-91666c59-4a69-460f-8cd3-9ddbff370ae5">nadV</tls:syn-func>
  <tls:sem-feat corresp="#uuid-dd37c44b-5a41-45e6-a045-090d47ae4923">time</tls:sem-feat>
