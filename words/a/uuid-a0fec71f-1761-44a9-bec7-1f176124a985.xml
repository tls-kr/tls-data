  <tls:syn-func corresp="#uuid-0ae78c50-f7f7-4ab0-bb28-9375998ac032">NP{N1=N2}</tls:syn-func>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
  <def>dwarf clown</def>
            </sense>
</entry>
