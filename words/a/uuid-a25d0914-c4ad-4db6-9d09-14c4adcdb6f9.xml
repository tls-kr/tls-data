  <tls:syn-func corresp="#uuid-e64a7a95-b54b-4c94-9d6d-f55dbf079701">vt(oN)</tls:syn-func>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
  <def>bid formally welcome a contextually determinate person</def>
            </sense>
<sense xml:id="uuid-92c73da5-4a6d-49cc-bacf-410dc9efefe2" n="31">
<gramGrp>
