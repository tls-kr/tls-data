    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="MARRY" tls:concept-id="uuid-4affa3a2-bf8d-40e9-93ea-02ba10fcd7a0" xml:id="uuid-64191e2e-c9e7-4c61-9d1c-d7a511cc8af0" n="23">
            <form corresp="#uuid-7b9cd6ea-de5b-4d57-9be5-7ecf3faf893c">
<orth>妻</orth>
