<gramGrp>
                    <pos/>
  <tls:syn-func corresp="#uuid-c8226be5-feb8-49cc-997b-f32ca5bbaac6">vttoN1:+vtoN2</tls:syn-func>
                </gramGrp>
  <def>provide N1 with the wife N2; marry N1 off to N2</def>
            </sense>
<sense xml:id="uuid-c7ea3367-4439-4fb5-9f8c-edbd9cb50f43" n="2">
<gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-fbfb2371-2537-4a99-a876-41b15ec2463c">vtoN</tls:syn-func>
  <tls:sem-feat corresp="#uuid-325659dc-d341-41ef-9a01-653dd72dfcee">N=bridegroom</tls:sem-feat>
                </gramGrp>
  <def>marry off to one's own daughter</def>
            </sense>
<sense xml:id="uuid-41bf7ded-4578-4b45-bf7d-ed45782b4537" resp="#CH" tls:created="2022-08-16T21:07:26.76+09:00" n="1">
