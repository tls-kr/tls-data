    <entry xmlns:tls="http://hxwd.org/ns/1.0" tls:concept="PRECIOUS" tls:concept-id="uuid-612c914e-1b4e-44d3-a48a-3f717c51432b" xml:id="uuid-7299f116-856e-42d0-93b1-604d312aa9de" n="18">
