                <pron xml:lang="zh-Latn-x-pinyin">fú zhāng</pron>
                <pron xml:lang="zh-x-oc">bɯɡ kjaŋ</pron>
                <pron xml:lang="zh-x-mc">buk tɕi̯ɐŋ</pron>
