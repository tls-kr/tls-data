<gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-76be1df4-3d73-4e5f-bbc2-729542645bc8">nab</tls:syn-func>
  <tls:sem-feat corresp="#uuid-f55cff2f-f0e3-4f08-a89c-5d08fcf3fe89">act</tls:sem-feat>
                    <usg type="warring-states-currency">4</usg>
                </gramGrp>
  <def>disrespect, contempt</def>
            </sense>
