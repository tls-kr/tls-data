                    <seg xml:id="KR5c0126_tls_004-5a.4" modified="2023-02-19T21:39:14.111+09:00" resp-change="#CH">大多政<c n="，"/>
                    </seg>
                    <seg xml:id="KR3f0040_WYG_000-d11fw.s1" state="locked" modified="2023-02-28T03:28:14.569+09:00" resp-change="#ziwei.qiu.qiu" change="deletion">
