                        <c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.9" resp="#CH" modified="2022-07-28T22:20:11.675+09:00" resp-change="#CH">《春秋》 學之美者也，</seg>
                    <seg xml:id="KR3j0010_tls_013-5a.8" resp="#CH" modified="2022-07-28T22:20:23.681+09:00" resp-change="#CH" change="deletion"/>
