                <p xml:id="del-2022-07-start">
                    <seg xml:id="CH7x2042_CHANT_060-9a.16" modified="2022-07-09T19:04:28.572+09:00" resp-change="#CH">申失雉經<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-4a.22" modified="2022-07-28T20:45:02.676+09:00" resp-change="#CH" change="deletion">夫夏<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-4a.23" modified="2022-07-28T20:46:14.025+09:00" resp-change="#CH">商之衰也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.8" modified="2022-07-28T22:19:37.38+09:00" resp-change="#CH">
                        <c n="《"/>春秋<c n="》"/>
                        <c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.9" modified="2022-07-28T22:19:46.404+09:00" resp-change="#CH">學之美者也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.7" modified="2022-07-28T22:19:59.215+09:00" resp-change="#CH">
                        <c n="《"/>詩<c n="》"/>
                        <c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.9" resp="#CH" modified="2022-07-28T22:20:11.675+09:00" resp-change="#CH">《春秋》 學之美者也，</seg>
                    <seg xml:id="KR3j0010_tls_013-5a.8" resp="#CH" modified="2022-07-28T22:20:23.681+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-5a.7" resp="#CH" modified="2022-07-28T22:20:27.288+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-5a.14" modified="2022-07-28T22:30:39.741+09:00" resp-change="#CH">
                        <c n="《"/>春秋<c n="》"/>為古之道而貴之<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.13" modified="2022-07-28T22:30:49.038+09:00" resp-change="#CH">以<c n="《"/>詩<c n="》"/>
                        <c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.14" resp="#CH" modified="2022-07-28T22:31:03.582+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-5a.15" modified="2022-07-28T22:31:21.093+09:00" resp-change="#CH">又有未作<c n="《"/>詩<c n="》"/>
                        <c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.16" modified="2022-07-28T22:31:34.505+09:00" resp-change="#CH">
                        <c n="《"/>春秋<c n="》"/>之時<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.15" resp="#CH" modified="2022-07-28T22:31:44.991+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-6a.20" modified="2022-07-29T00:40:25.122+09:00" resp-change="#CH">蔡之罪<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-6a.19" modified="2022-07-29T00:40:30.055+09:00" resp-change="#CH">誅管<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-6a.20" resp="#CH" modified="2022-07-29T00:40:36.987+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-8a.36" modified="2022-07-29T19:12:54.181+09:00" resp-change="#CH">射<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-8a.36" resp="#CH" modified="2022-07-29T19:13:01.962+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-8a.35" modified="2022-07-29T19:13:20.825+09:00" resp-change="#CH">連弩以<c n="（"/>󰋱<c n="）"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-9a.2" modified="2022-07-29T19:32:29.231+09:00" resp-change="#CH">夫神農<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-9a.3" modified="2022-07-29T19:32:48.683+09:00" resp-change="#CH" change="deletion">伏犧不施賞罰而民不為非<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-11a.22" modified="2022-07-30T00:08:41.768+09:00" resp-change="#CH">文武而弗行也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-11a.21" modified="2022-07-30T00:08:53.67+09:00" resp-change="#CH">今儒墨者稱三代<c n="、"/>
                    </seg>
                    <seg xml:id="KR1h0004_tls_001-15a.9" modified="2022-07-30T00:18:51.363+09:00" resp-change="#CH">道<c n="，"/>
                    </seg>
                    <seg xml:id="KR1h0004_tls_001-15a.9" resp="#CH" modified="2022-07-30T00:18:58.112+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR1h0004_tls_001-15a.8" modified="2022-07-30T00:19:08.658+09:00" resp-change="#CH">未若貧而樂</seg>
                    <seg xml:id="KR3j0010_tls_013-13a.49" modified="2022-07-30T07:18:32.627+09:00" resp-change="#CH">秦青<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.49" resp="#CH" modified="2022-07-30T07:18:38.738+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-13a.48" modified="2022-07-30T07:18:47.014+09:00" resp-change="#CH">及至韓娥<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.50" modified="2022-07-30T07:18:55.79+09:00" resp-change="#CH">薛談之謳<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.50" resp="#CH" modified="2022-07-30T07:19:02.043+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-13a.48" resp="#CH" modified="2022-07-30T07:19:11.9+09:00" resp-change="#CH">及至韓娥、秦青、</seg>
                    <seg xml:id="KR3j0010_tls_013-13a.52" modified="2022-07-30T07:21:14.402+09:00" resp-change="#CH">曼聲之歌<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.52" resp="#CH" modified="2022-07-30T07:21:18.888+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-13a.51" modified="2022-07-30T07:21:26.391+09:00" resp-change="#CH">侯同<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.68" modified="2022-07-30T07:29:21.339+09:00" resp-change="#CH">吳起而亡西河<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.68" resp="#CH" modified="2022-07-30T07:29:28.265+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-13a.67" modified="2022-07-30T07:29:35.256+09:00" resp-change="#CH">故魏兩用樓翟<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.72" modified="2022-07-30T07:34:03.581+09:00" resp-change="#CH">召公奭而王<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-13a.72" resp="#CH" modified="2022-07-30T07:34:07.868+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-13a.71" modified="2022-07-30T07:34:13.557+09:00" resp-change="#CH">文王兩用呂望<c n="、"/>
                    </seg>
                </p>
