                    <seg xml:id="KR3a0009_tls_002-4a.2" modified="2024-07-04T02:56:46.062+09:00" resp-change="#CH">或問<c n="："/>
                        <c n="「"/>交五聲<c n="、"/>十二律也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3a0009_tls_002-4a.7" modified="2024-07-04T04:40:15.424+09:00" resp-change="#CH">請問<c n="「"/>本<c n="」"/>
