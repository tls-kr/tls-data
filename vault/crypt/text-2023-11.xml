                <p xml:id="del-2023-11-start">
                    <seg xml:id="KR5c0057_tls_012-1a.11" modified="2023-11-01T00:15:37.52+09:00" resp-change="#CH">之治也<c n="，"/>為腹</seg>
                    <seg xml:id="KR5c0057_tls_024-1a.9" modified="2023-11-01T02:38:40.627+09:00" resp-change="#CH">其在道也<c n="，"/>曰</seg>
                    <seg xml:id="KR5c0057_tls_024-1a.10" modified="2023-11-01T02:38:48.577+09:00" resp-change="#CH">
                        <c n="「"/>餘食<c n="、"/>贅行<c n="」"/>
                        <c n="。"/>
                    </seg>
                    <seg xml:id="KR5c0057_tls_029-1a.5" modified="2023-11-01T04:50:57.1+09:00" resp-change="#CH">天下<c n="、"/>神器<c n="，"/>
                    </seg>
                    <seg xml:id="KR5c0057_tls_031-1a.3" modified="2023-11-01T05:24:50.323+09:00" resp-change="#CH">夫隹兵者<c n="、"/>不祥之器也<c n="，"/>
                    </seg>
                    <seg xml:id="KR5c0057_tls_031-1a.8" modified="2023-11-01T05:26:00.16+09:00" resp-change="#CH">故兵者<c n="、"/>不祥之器也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_001-6a.19" modified="2023-11-02T08:11:33.091+09:00" resp-change="#CH">繳不若網<c n="，"/>網不若無形之像<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_001-6a.19" resp="#CH" modified="2023-11-02T08:13:06.263+09:00" resp-change="#CH">繳不若網，</seg>
                    <seg xml:id="KR3j0010_tls_001-13a.37" modified="2023-11-02T19:37:46.954+09:00" resp-change="#CH">也<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_001-13a.36" modified="2023-11-02T19:38:19.303+09:00" resp-change="#CH">因物之所為</seg>
                    <seg xml:id="KR3j0010_tls_001-13a.37" resp="#CH" modified="2023-11-02T19:40:12.229+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_001-14a.48" modified="2023-11-02T22:01:30.846+09:00" resp-change="#CH">生之榦也<c n="，"/>
                        <c n="（"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_001-14a.49" modified="2023-11-02T22:01:36.853+09:00" resp-change="#CH">而<c n="）"/>堅強者<c n="、"/>
                    </seg>
                </p>
