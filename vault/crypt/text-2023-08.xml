                    <seg xml:id="KR3f0047_WYG_003-d4100p59.s249" type="comm" state="locked" modified="2023-08-02T22:44:12.669+09:00" resp-change="#Valerie.Kiel" change="deletion">
                        <lb/> <space n="　" quantity="1"/>幾何原本卷三</seg>
                    <seg xml:id="KR5c0057_tls_049-1a.12" modified="2023-08-02T22:59:26.794+09:00" resp-change="#CH">天下也<c n="，"/>歙歙焉</seg>
                    <seg xml:id="KR5c0057_tls_049-1a.11" modified="2023-08-02T22:59:36.352+09:00" resp-change="#CH">聖人之在</seg>
                    <seg xml:id="KR3j0010_tls_006-2a.11" modified="2023-08-03T16:36:19.919+09:00" resp-change="#CH">
                        <c n="「"/>余在<c n="，"/>天下</seg>
                    <seg xml:id="KR3j0010_tls_006-2a.12" modified="2023-08-03T16:36:31.543+09:00" resp-change="#CH">誰敢害吾志者<c n="！"/>
                        <c n="」"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_012-57a.3" modified="2023-08-03T22:07:42.379+09:00" resp-change="#CH">
                        <c n="「"/>寡人伐紂天下<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_012-57a.4" modified="2023-08-03T22:07:56.51+09:00" resp-change="#CH">是臣殺其主而下伐其上也<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_014-33a.23" modified="2023-08-04T17:31:49.68+09:00" resp-change="#CH">百姓不怨則民用可得<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_014-33a.24" modified="2023-08-04T17:31:59.873+09:00" resp-change="#CH">諸侯弗備<c n="；"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_014-36a.14" modified="2023-08-04T17:38:24.289+09:00" resp-change="#CH">焉可以託天下也<c n="。"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_015-1a.31" modified="2023-08-04T18:11:55.911+09:00" resp-change="#CH">萬民󰋲動<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_020-10a.23" modified="2023-08-04T22:27:31.458+09:00" resp-change="#CH">則天下聽从<c n="；"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_020-28a.27" modified="2023-08-05T00:03:12.213+09:00" resp-change="#CH">大利也<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_020-28a.26" modified="2023-08-05T00:03:24.351+09:00" resp-change="#CH">天下<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_020-28a.27" resp="#CH" modified="2023-08-05T00:03:37.616+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3a0002_tls_018-2a.120" modified="2023-08-05T17:12:32.311+09:00" resp-change="#CH">不可以有竊天下也<c n="。"/>
                        <c n="（"/>
                    </seg>
                    <seg xml:id="KR3a0002_tls_018-2a.121" modified="2023-08-05T17:12:38.737+09:00" resp-change="#CH">可以<c n="）"/>奪之者可以有國<c n="，"/>
                    </seg>
                    <seg xml:id="KR5c0057_tls_001-1a.7" modified="2023-08-10T00:55:40.863+09:00" resp-change="#CH">無名<c n="、"/>天地之始<c n="，"/>
                    </seg>
                    <seg xml:id="KR5c0057_tls_001-1a.8" modified="2023-08-10T00:55:47.379+09:00" resp-change="#CH">有名<c n="、"/>萬物之母<c n="，"/>
                    </seg>
                    <seg xml:id="KR5c0126_tls_013-2a.28" modified="2023-08-11T21:28:58.862+09:00" resp-change="#CH">一心定而王天下<c n="；"/>
                    </seg>
                    <seg xml:id="KR5c0126_tls_033-17a.2" modified="2023-08-12T21:36:23.507+09:00" resp-change="#CH">惠施以此為大觀於天下</seg>
                    <seg xml:id="KR5c0126_tls_033-17a.3" modified="2023-08-12T21:36:35.608+09:00" resp-change="#CH">而曉辯者<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-5a.19" resp="#CH" modified="2023-08-31T18:24:30.091+09:00" resp-change="#CH">誦先王之《詩》、《書》 ，</seg>
                </p>
