                    <seg xml:id="KR3j0010_tls_013-25a.2" modified="2022-08-02T05:56:42.486+09:00" resp-change="#CH">夫堯<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-25a.2" resp="#CH" modified="2022-08-02T05:57:08.088+09:00" resp-change="#CH">夫堯、舜、湯、</seg>
                    <seg xml:id="KR3j0010_tls_013-25a.4" modified="2022-08-02T05:57:17.398+09:00" resp-change="#CH" change="deletion">武<c n="，"/>世主之隆也<c n="；"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-26a.9" modified="2022-08-02T06:37:23.559+09:00" resp-change="#CH">而不知其大略<c n="，"/>
                    </seg>
