                    <seg xml:id="KR3j0010_tls_013-26a.8" modified="2022-08-02T06:37:38.771+09:00" resp-change="#CH">事之洿辱<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-27a.28" modified="2022-08-02T06:59:56.227+09:00" resp-change="#CH">易牙<c n="，"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-27a.27" modified="2022-08-02T07:00:01.241+09:00" resp-change="#CH">臾兒<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-27a.29" modified="2022-08-02T07:00:11.38+09:00" resp-change="#CH">淄<c n="、"/>澠之水合者<c n="，"/>
