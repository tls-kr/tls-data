                    <seg xml:id="KR3j0010_tls_013-20a.6" resp="#CH" modified="2022-08-02T01:09:09.236+09:00" resp-change="#CH" change="deletion"/>
                    <seg xml:id="KR3j0010_tls_013-20a.5" modified="2022-08-02T01:09:16.848+09:00" resp-change="#CH">昔者萇弘<c n="、"/>
                    </seg>
                    <seg xml:id="KR3j0010_tls_013-25a.3" modified="2022-08-02T05:56:27.449+09:00" resp-change="#CH">舜<c n="、"/>湯<c n="、"/>
