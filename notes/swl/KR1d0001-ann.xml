                                <pron xml:lang="zh-Latn-x-pinyin">xīng chén</pron>
                            </form>
                            <sense corresp="#uuid-5a44d6b2-2e2d-44dd-a4ec-4a1a3f6e9a38">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-0e71a24c-2529-482a-a575-a4f143a9890b">NP{N1&amp;N2}</tls:syn-func>
  <tls:sem-feat corresp="#uuid-f8182437-4c38-4cc9-a6f8-b4833cdea2ba">nonreferential</tls:sem-feat>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
                                <def>stars and constellations, heavenly bodies</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2019-03-26T13:08:54.554+09:00">
