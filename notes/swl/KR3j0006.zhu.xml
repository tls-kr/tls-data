<sense corresp="#uuid-67612288-57ca-4862-9cab-87517ae66f41">
<gramGrp>
<pos>N</pos>
<tls:syn-func corresp="#uuid-76be1df4-3d73-4e5f-bbc2-729542645bc8">nab</tls:syn-func>
<tls:sem-feat corresp="#uuid-2a66fc1c-6671-47d2-bd04-cfd6ccae64b8">stative</tls:sem-feat>
</gramGrp>
<def>level of attainment</def>
</sense>
</tls:ann>
<tls:ann concept="HUNDRED">
<link target="#KR3j0006_tls_001-1a.11 #uuid-63f74fb0-472a-4734-916f-1ab9a184b964"/>
