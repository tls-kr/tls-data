        <tls:srcline title="祖堂集" target="#KR6q0002_Yan_001-1011a.1" pos="1">毗舍羅種、首陀種。</tls:srcline>
        <tls:line title="祖堂集 (en)" src="Christoph Anderl">the Vaiśya caste, and the Śūdra caste.</tls:line>
    </tls:text>
    <form corresp="#uuid-d37854af-0991-4e94-b839-8fc9ecdf8264 #uuid-3c1879b8-3bd0-413a-95b5-69d2f36731e5 #uuid-73b6e4e2-147a-4ead-8d0b-386283e2a333 #uuid-b06a5597-6455-4c71-84d7-bdbfdd50264a" orig="毗舍羅種">
