                    <seg xml:id="KR1j0007_tls_001-1a.6">
                        <line>或典禮所制，</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="PRONOUN" concept-id="uuid-c1654743-3e55-4188-816b-511da251682f" xml:id="uuid-afb94ef8-8f99-4911-95df-ac008acedfb9">
                            <link target="#KR1j0007_tls_001-1a.6 #uuid-5f7e4c36-c515-43cb-b66f-6d9955539743"/>
                            <tls:text>
                                <tls:srcline title="釋名" target="#KR1j0007_tls_001-1a.6" pos="1">或典禮所制，</tls:srcline>
                                <tls:line title="釋名 (en)" src="">some (words) have been defined by the classics and by ritual propriety</tls:line>
                            </tls:text>
                            <form corresp="#uuid-7be571ca-f00b-41c6-b5eb-2c0b43e6bcd8" orig="或">
                                <orth>或</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">huò</pron>
                            </form>
                            <sense corresp="#uuid-5f7e4c36-c515-43cb-b66f-6d9955539743">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-da183583-38b2-44d1-8165-a48331d55847">npro.adV</tls:syn-func>
  <tls:sem-feat corresp="#uuid-090d194a-771e-4679-bea3-800692a6c1d5">repeated</tls:sem-feat>
                </gramGrp>
                                <def>some... others (often hard to distinguish from "either... or"); some..., the others</def>
                            </sense>
