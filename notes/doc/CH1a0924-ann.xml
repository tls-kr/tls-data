                                <pron xml:lang="zh-Latn-x-pinyin">sī mén</pron>
                            </form>
                            <sense corresp="#uuid-d5e8c4e1-54e4-4ed4-8ba4-5d327f363eaa">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-a8e89bab-49e1-4426-b230-0ec7887fd8b4">NP</tls:syn-func>
  <tls:sem-feat corresp="#uuid-5fae11b4-4f4e-441e-8dc7-4ddd74b68c2e">plural</tls:sem-feat>
                </gramGrp>
                                <def resp="#CH" updated="2022-09-09T18:40:13.649+09:00">private families (uninvolved in formal government); burghers</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2022-09-09T20:24:08.811+09:00">
