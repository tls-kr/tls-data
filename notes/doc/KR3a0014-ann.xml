                    <seg xml:id="KR3a0014_tls_007-31a.10">
                        <line>其唯君子乎？</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="ONLY" concept-id="uuid-72833a4e-d1ec-471f-806c-64c1e3317aaf" xml:id="uuid-55f017e6-0595-4f75-bcab-3cc6d0b58815">
                            <link target="#KR3a0014_tls_007-31a.10 #uuid-dda34042-0ee2-4970-8ece-a3c04ff45147"/>
                            <tls:text>
                                <tls:srcline title="中說" target="#KR3a0014_tls_007-31a.10" pos="1">其唯君子乎？</tls:srcline>
                                <tls:line title="中說 (en)" src="">this is given only to the superior person!”</tls:line>
                            </tls:text>
                            <form corresp="#uuid-4d6c7918-4df1-492f-95db-6e81913b1710 #uuid-da888da1-0d6e-4d5c-a711-60c1c713d85b" orig="">
                                <orth>其唯</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">qí wéi</pron>
                            </form>
                            <sense corresp="#uuid-dda34042-0ee2-4970-8ece-a3c04ff45147">
                                <gramGrp>
                                    <pos>V</pos>
                                    <tls:syn-func corresp="#">vt N{SUBJECT}</tls:syn-func>
                                </gramGrp>
                                <def>(presumably) only N is a case in point, only N is like this</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2020-01-01T20:42:02.194+09:00">
                                <respStmt>
                                    <resp>added and approved</resp>
                                    <name>CH</name>
                                </respStmt>
                            </tls:metadata>
                        </tls:ann>
                    </seg>
                    <seg xml:id="KR3a0014_tls_008-2a.13">
                        <line>可謂遠刑名矣。</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="GOVERN" concept-id="uuid-6e22b016-6b73-4e15-912c-79e55857f160" xml:id="uuid-8dc8dc2f-2947-4820-88dc-2f2947882032">
