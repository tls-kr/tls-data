                                <orth>丘邱坵</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">qiū</pron>
                                <pron xml:lang="zh-Latn-x-pinyin">qiū</pron>
                                <pron xml:lang="zh-Latn-x-pinyin"/>
                            </form>
                            <sense corresp="#uuid-d643cd63-a388-4bf4-870b-c73486b75736">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-8717712d-14a4-4ae2-be7a-6e18e61d929b">n</tls:syn-func>
                    <usg type="warring-states-currency">2</usg>
                </gramGrp>
                                <def>dialect: burial mound, often that of an emperor</def>
                            </sense>
