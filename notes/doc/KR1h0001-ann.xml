                                <tls:srcline title="孟子" target="#KR1h0001_tls_002-49a.3" pos="2">「湯放桀，</tls:srcline>
                                <tls:line title="孟子 (en)" src="David Sehnal; D.C. Lau">"Tāng banished Jié</tls:line>
                            </tls:text>
                            <form corresp="#uuid-7d069442-b3c1-4957-980b-3f51b76ece0c" orig="">
