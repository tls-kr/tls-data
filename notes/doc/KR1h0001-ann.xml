                            <tls:text>
                                <tls:srcline title="孟子" target="#KR1h0001_tls_002-51a.1" pos="3">「於傳有之。」</tls:srcline>
                                <tls:line title="孟子 (en)" src="David Sehnal; D.C. Lau">"It exists in records."</tls:line>
                            </tls:text>
                            <form corresp="#uuid-d572d9db-b1f8-44da-a217-c1525104e8b8" orig="">
                                <orth>傳</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">zhuàn</pron>
                            </form>
                            <sense corresp="#uuid-0b0e2410-0040-4c33-bf00-be9c7aed5822">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-8717712d-14a4-4ae2-be7a-6e18e61d929b">n</tls:syn-func>
                    <usg type="warring-states-currency">4</usg>
                </gramGrp>
                                <def>traditional account based on pre-existing sources, tradition</def>
                            </sense>
                            <tls:metadata resp="#dsehnal" created="2019-10-12T04:27:30.152+09:00">
                                <respStmt>
