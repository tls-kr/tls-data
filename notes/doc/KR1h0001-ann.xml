                            </form>
                            <sense corresp="#uuid-7b2609ab-28b1-45d2-8009-93c1804c3a4a">
                                <gramGrp>
                                    <pos>N</pos>
                                    <tls:syn-func corresp="#uuid-c43c0bab-2810-42a4-a6be-e4641d9b6632">NPpr</tls:syn-func>
                                </gramGrp>
                                <def>King Wu, founder of the Zhou Dynasty</def>
                            </sense>
