                                <respStmt>
                                    <resp>added</resp>
                                    <name>dsehnal</name>
                                </respStmt>
                            </tls:metadata>
                        </tls:ann>
                    </seg>
                    <seg xml:id="KR1h0001_tls_002-51a.1">
                        <line>「於傳有之。」</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="DOCUMENT" concept-id="uuid-42b11726-0033-4229-8329-4e0e443f1fa9" xml:id="uuid-a244ef12-17bf-450c-a2b6-ddd4779e19cb">
