                            <tls:text>
                                <tls:srcline title="南華真經註疏" target="#KR5c0139_tls_000-003cs.9.1.14.9" pos="1">古有殺青爲簡,</tls:srcline>
                                <tls:line title="Translation of 南華真經註疏 into (English)" transl-id="KR5c0139-en-2339bd49" src="TLS Project">In ancient times, green bamboo was scraped to make tablets,</tls:line>
                            </tls:text>
                            <form corresp="#uuid-42906930-31d1-4360-baa5-ed14eb90c285" orig="">
                                <orth>殺</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">shā</pron>
                            </form>
                            <sense corresp="#uuid-f4bfb47b-c409-44a2-a48f-28d0d7441723">
                                <gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-fbfb2371-2537-4a99-a876-41b15ec2463c">vtoN</tls:syn-func>
                    <usg type="warring-states-currency">3</usg>
                </gramGrp>
                                <def>cut up, cut into convenient pieces</def>
                            </sense>
