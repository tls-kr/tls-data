                                <pron xml:lang="zh-Latn-x-pinyin">tài shǒu</pron>
                            </form>
                            <sense corresp="#uuid-7e70f952-4813-4a0e-9de6-505fac110b9e">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-e2ece349-6f09-49f0-be4e-7b7c66094e6f">NP(post-N)</tls:syn-func>
                </gramGrp>
                                <def>governor of a contextually determinate place N</def>
                            </sense>
