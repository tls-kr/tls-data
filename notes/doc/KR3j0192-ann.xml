                            </form>
                            <sense corresp="#uuid-573815e2-5f5e-4d19-ae86-dbb9da78e4b1">
                                <gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-98f2ce75-ae37-4667-90ff-f418c4aeaa33">VPtoN</tls:syn-func>
                    <usg type="warring-states-currency">5</usg>
                </gramGrp>
                                <def resp="#CH" updated="2022-04-14T21:23:08.508+09:00">may be properly compared with, be comparable to the case of (typically but not always with understood subject)</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2022-04-14T21:25:21.325+09:00">
                                <respStmt>
                                    <resp>added</resp>
                                    <name notBefore="2022-04-14T21:25:21.325+09:00">CH</name>
                                </respStmt>
                            </tls:metadata>
                        </tls:ann>
                    </seg>
                    <seg xml:id="KR3j0192_KR3j0192_002-17a.4">
