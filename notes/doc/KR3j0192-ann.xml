                                <tls:srcline title="新論-漢-桓譚" target="#KR3j0192_KR3j0192_002-26a.6" pos="1">人物蒙美盛而生，
                    </tls:srcline>
                                <tls:line title="新論-漢-桓譚 (en)" transl-id="KR3j0192-en-d16c241a" src="???">people were born with beauty and wealth.</tls:line>
                            </tls:text>
                            <form corresp="#uuid-21fa0930-1ebd-4609-9c0d-ef7ef7a2723f #uuid-920cdc9d-a13f-4145-b5d6-a18eda88b3cc" orig="">
                                <orth>人物</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">rén wù</pron>
                            </form>
                            <sense corresp="#uuid-686116d0-ec60-498c-a116-d0ec60698ced">
                                <gramGrp>
                                    <pos>N</pos>
                                    <tls:syn-func corresp="#uuid-a8e89bab-49e1-4426-b230-0ec7887fd8b4">NP</tls:syn-func>
                                    <tls:sem-feat corresp="#uuid-f8182437-4c38-4cc9-a6f8-b4833cdea2ba">nonreferential</tls:sem-feat>
                                </gramGrp>
                                <def resp="#CH" updated="2021-07-12T18:20:49.732+09:00">certain individuals; human individuals; person (as having it's own features)</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2021-07-12T18:26:42.509+09:00">
                                <respStmt>
                                    <resp>added</resp>
                                    <name notBefore="2021-07-12T18:26:42.509+09:00">CH</name>
                                </respStmt>
                            </tls:metadata>
                        </tls:ann>
                    </seg>
                    <seg xml:id="KR3j0192_KR3j0192_002-15a.7">
                        <line>譬如市肆，
                    </line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="RESEMBLE" concept-id="uuid-031779e4-5e29-4947-9e9f-b5edc803c7be" xml:id="uuid-bcc8e640-bc6f-4f21-88e6-40bc6faf2172">
