                    <seg xml:id="CH2b1581_CHANT_003-8a.6">
                        <line>事事悉如昔。
                    </line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="ALL" concept-id="uuid-78fc2812-4bf3-4e0a-8efe-78907250d397" xml:id="uuid-1cfffaff-8e9b-4e30-bffa-ff8e9b3e3042">
                            <link target="#CH2b1581_CHANT_003-8a.6 #uuid-3a3b6116-2e1d-4fbd-85de-4513a959258a"/>
                            <tls:text>
                                <tls:srcline title="陶淵明集十一卷" target="#CH2b1581_CHANT_003-8a.6" pos="3">事事悉如昔。
                    </tls:srcline>
                                <tls:line title="Translation of 陶淵明集十一卷 into (English)" transl-id="CH2b1581-en-62f20d4a" src="James Robert Hightower">And every thing is as it was: </tls:line>
                            </tls:text>
                            <form corresp="#uuid-0ab69ce6-1729-4196-aa98-cffbbeebac63" orig="">
                                <orth>悉</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">xī</pron>
                            </form>
                            <sense corresp="#uuid-3a3b6116-2e1d-4fbd-85de-4513a959258a">
                                <gramGrp>
                    <pos>V</pos>
  <tls:syn-func corresp="#uuid-2a0ded86-3b04-4488-bb7a-3efccfa35844">vadV</tls:syn-func>
  <tls:sem-feat corresp="#uuid-d1cf7a99-5f60-4ba5-ac4d-ce56db9366bd">quantifier</tls:sem-feat>
                </gramGrp>
