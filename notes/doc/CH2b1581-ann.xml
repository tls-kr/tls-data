                    <seg xml:id="CH2b1581_CHANT_003-39a.16">
                        <line>餒也已矣夫，
                    </line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="MODAL PARTICLES" concept-id="uuid-3ecbc80a-34eb-4b0f-ae75-a2902596aaca" xml:id="uuid-a0aabad7-d7b7-4c41-aaba-d7d7b77c4167">
                            <link target="#CH2b1581_CHANT_003-39a.16 #uuid-ae102a1e-7260-4422-902a-1e7260b4223d"/>
                            <tls:text>
                                <tls:srcline title="陶淵明集十一卷" target="#CH2b1581_CHANT_003-39a.16" pos="2">餒也已矣夫，
                    </tls:srcline>
                                <tls:line title="Translation of 陶淵明集十一卷 into (English)" transl-id="CH2b1581-en-62f20d4a" src="James Robert Hightower">To hunger is my lot, so be it!</tls:line>
                            </tls:text>
                            <form corresp="#uuid-208b48d4-5b38-4edb-8418-80f4dcff11e3 #uuid-e799b325-78d4-4326-a46d-ca3498ecce7a #uuid-644760a0-b567-4543-90dd-32afbfa9849c #uuid-c21f7a99-de70-44d2-a0e2-4266db4736bd" orig="">
                                <orth>也已矣夫</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">yě yǐ yǐ fú</pron>
                            </form>
                            <sense corresp="#uuid-ae102a1e-7260-4422-902a-1e7260b4223d">
                                <gramGrp>
                                    <pos>P</pos>
                                    <tls:syn-func corresp="#uuid-04c5d26e-838a-4267-a260-5926111fe077">PPpostadS</tls:syn-func>
                                </gramGrp>
                                <def>states a fact, emphatically, as currently relevant, appealing then to the reader that this is true</def>
                            </sense>
                            <tls:metadata resp="#CH" created="2022-02-24T01:27:42.93+09:00">
