                    </seg>
                <seg xml:id="KR6d0001_T_006-0048a.16">
                        <line>三千世界聲。</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="UNIVERSE" concept-id="uuid-383dc31a-fb6a-4c5a-9182-83a8cb967831" xml:id="uuid-4ebec10a-005b-4887-bec1-0a005b7887eb">
                            <link target="#KR6d0001_T_006-0048a.16 #uuid-8fa9d2cd-f346-4dc7-bf7f-d883c28a15f5"/>
                            <tls:text>
                                <tls:srcline title="妙法蓮華經" target="#KR6d0001_T_006-0048a.16" pos="1">三千世界聲。</tls:srcline>
                                <tls:line title="妙法蓮華經 (en)" src="WATSON 1993 ">the sounds of the three thousand worlds,</tls:line>
                            </tls:text>
                            <form corresp="#uuid-3b81e026-2aee-45cd-b686-7bab8c7046b3 #uuid-f8fe7cb5-faea-4943-b003-8338a85bac09 #uuid-0a2970a8-0d00-4baf-9651-be47b9df2279 #uuid-b079fe55-8453-426a-bdcb-61d45134edeb" orig="">
                                <orth>三千世界</orth>
                                <pron xml:lang="zh-Latn-x-pinyin">sān qiān shì jiè</pron>
                            </form>
                            <sense corresp="#uuid-8fa9d2cd-f346-4dc7-bf7f-d883c28a15f5">
                                <gramGrp>
                    <pos>N</pos>
  <tls:syn-func corresp="#uuid-a8e89bab-49e1-4426-b230-0ec7887fd8b4">NP</tls:syn-func>
                </gramGrp>
                                <def>BUDDH: a great chiliocosm (see 三千大千世界)</def>
                            </sense>
                            <tls:metadata resp="#tls" created="2019-08-28T16:28:09.335+09:00">
                                <respStmt>
                                    <resp>added</resp>
                                    <name>tls</name>
                                </respStmt>
                            </tls:metadata>
                        </tls:ann>
                    </seg>
                    <seg xml:id="KR6d0001_T_006-0050a.14">
                        <line>三千世界中，</line>
                        <tls:ann xmlns:tls="http://hxwd.org/ns/1.0" concept="UNIVERSE" concept-id="uuid-383dc31a-fb6a-4c5a-9182-83a8cb967831" xml:id="uuid-df76b3bc-e622-4d81-b6b3-bce6222d8106">
                            <link target="#KR6d0001_T_006-0050a.14 #uuid-8fa9d2cd-f346-4dc7-bf7f-d883c28a15f5"/>
                            <tls:text>
                                <tls:srcline title="妙法蓮華經" target="#KR6d0001_T_006-0050a.14" pos="1">三千世界中，</tls:srcline>
                                <tls:line title="妙法蓮華經 (en)" src="WATSON 1993 ">Within the three thousand worlds</tls:line>
                            </tls:text>
                            <form corresp="#uuid-3b81e026-2aee-45cd-b686-7bab8c7046b3 #uuid-f8fe7cb5-faea-4943-b003-8338a85bac09 #uuid-0a2970a8-0d00-4baf-9651-be47b9df2279 #uuid-b079fe55-8453-426a-bdcb-61d45134edeb" orig="">
