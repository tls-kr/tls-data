                            <tls:text>
                                <tls:srcline title="妙法蓮華經" target="#KR6d0001_T_004-0036c.15" pos="4">婆羅門居士，</tls:srcline>
                                <tls:line title="妙法蓮華經 (en)" src="WATSON 1993 ">Brahmans and householders,</tls:line>
                            </tls:text>
                            <form corresp="#uuid-a6dcd777-5670-4662-abdb-4768856163a8 #uuid-fb89a673-a23b-40ad-ab82-7b44c4b3995e" orig="">
