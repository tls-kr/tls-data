<head>FOREIGNER</head>
    <list type="altnames">
<item>ALIEN</item>
<item>NONNATIVE</item>
<item>STRANGER</item>
<item>OUTSIDER</item>
<item>IMMIGRANT</item>
<item>LANDED IMMIGRANT</item>
<item>REFUGEE</item>
