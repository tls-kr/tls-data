<head>FORD</head>
    <list type="altnames">
<item>CROSSING PLACE</item>
<item>CROSSING</item>
<item>SHALLOW PLACE</item>
    </list>
    <list type="translations">
        <item xml:lang="zh">津</item>
        <item xml:lang="och">津</item>
