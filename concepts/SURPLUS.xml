<head>SURPLUS</head>
    <list type="altnames">
<item>SUPERFLUOUS</item>
<item>EXCESS</item>
<item>SURFEIT</item>
<item>SUPERABUNDANCE</item>
<item>SUPERFLUITY</item>
<item>OVERSUPPLY</item>
<item>GLUT</item>
