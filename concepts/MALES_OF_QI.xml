<entry type="word" xml:id="uuid-6a653c96-f848-4ce6-9e81-5ab3264ff2bd">
            <form corresp="#uuid-4d1d694e-fc29-4b9f-ab11-e70d1b7c872b #uuid-1e24a96a-b3b4-4016-ab95-0acdb72d45d7 #uuid-79ed2be2-281e-45e3-bede-3c6bae832d28">
