<head>BARK</head>
    <list type="altnames">
<item>WOOF</item>
<item>YAP</item>
<item>YELP</item>
<item>BAY</item>
    </list>
    <list type="translations">
        <item xml:lang="zh">吠</item>
        <item xml:lang="och">吠</item>
