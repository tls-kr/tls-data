<item>SWAY</item>
<item>TRIP</item>
<item>TWIRL</item>
<item>WHIRL</item>
<item>PIROUETTE</item>
<item>GYRATE</item>
<item>INFORMAL BOP</item>
<item>DISCO</item>
<item>ROCK</item>
<item>BOOGIE</item>
<item>SHAKE A LEG</item>
<item>HOOF IT</item>
<item>CUT A/THE RUG</item>
<item>TRIP THE LIGHT FANTASTIC</item>
<item>GET DOWN</item>
<item>MOSH</item>
