<head>SOMETIMES</head><list type="altnames">
<item>AT TIMES</item>
<item>SOMETIMES</item>
<item>FROM TIME TO TIME</item>
<item>(EVERY) NOW AND THEN</item>
<item>(EVERY) NOW AND AGAIN</item>
<item>AT TIMES</item>
<item>EVERY SO OFTEN</item>
<item>(EVERY) ONCE IN A WHILE</item>
<item>ON OCCASION</item>
<item>PERIODICALLY</item>
<item>AT INTERVALS</item>
<item>IRREGULARLY</item>
<item>SPORADICALLY</item>
<item>INFREQUENTLY</item>
<item>INTERMITTENTLY</item>
