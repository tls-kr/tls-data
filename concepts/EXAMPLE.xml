<head>EXAMPLE</head>
    <list type="altnames">
<item>SPECIMEN</item>
<item>SAMPLE</item>
<item>EXEMPLAR</item>
<item>EXEMPLIFICATION</item>
<item>INSTANCE</item>
<item>CASE</item>
<item>ILLUSTRATION</item>
<item>CASE IN POINT</item>
    </list>
    <list type="translations">
        <item xml:lang="zh">例子</item>
        <item xml:lang="och">例</item>
