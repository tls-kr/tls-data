<head>INFINITE</head>
    <list type="altnames">
<item>BOUNDLESS</item>
<item>UNBOUNDED</item>
<item>UNLIMITED</item>
<item>LIMITLESS</item>
<item>NEVER-ENDING</item>
<item>INTERMINABLE</item>
<item>IMMEASURABLE</item>
<item>FATHOMLESS</item>
<item>IMPONDERABLE</item>
<item>EXTENSIVE</item>
<item>VAST</item>
<item>IMMENSE</item>
<item>GREAT</item>
<item>HUGE</item>
