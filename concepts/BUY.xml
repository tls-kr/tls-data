<head>BUY</head>
    <list type="altnames">
<item>PURCHASE</item>
<item>ACQUIRE</item>
<item>OBTAIN</item>
<item>GET</item>
<item>PICK UP</item>
<item>TAKE</item>
<item>PROCURE</item>
<item>PAY FOR</item>
<item>INVEST IN</item>
<item>GET HOLD OF</item>
<item>SNATCH UP</item>
<item>SNAP UP</item>
<item>GRAB</item>
