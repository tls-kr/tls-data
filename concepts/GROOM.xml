<head>GROOM</head>
    <list type="altnames">
<item>STABLE HAND</item>
<item>STABLEMAN</item>
<item>STABLE BOY</item>
<item>STABLE GIRL</item>
<item>HISTORICAL EQUERRY</item>
    </list>
    <list type="translations">
        <item xml:lang="zh">馬夫</item>
        <item xml:lang="och">圉人</item>
